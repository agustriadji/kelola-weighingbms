{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typeorm": "node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm -- migration:run -d ./typeorm-cli.js",
    "migration:revert": "npm run typeorm -- migration:revert -d ./typeorm-cli.js",
    "migration:create": "npm run typeorm -- migration:create -d ./typeorm-cli.js src/database/migrations/NewMigration",
    "migration:generate": "npm run typeorm -- migration:generate -d ./typeorm-cli.js src/database/migrations/AutoGen",
    "seed": "node scripts/seed.js",
    "db:setup": "npm run migration:run && npm run seed",
    "cron": "NODE_ENV=development ENABLE_SAP_CRON=true node worker.js",
    "start:cron": "ENABLE_SAP_CRON=true node worker.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run cron\""
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.6.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.474.0",
    "next": "14.2.22",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "radix-ui": "^1.1.2",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^10.1.0",
    "reflect-metadata": "^0.2.2",
    "remark-gfm": "^4.0.1",
    "typeorm": "^0.3.27",
    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^9.2.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.22",
    "ignore-loader": "^0.1.2",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typeorm-ts-node-commonjs": "^0.3.20",
    "typescript": "^5.6.3"
  }
}
